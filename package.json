{
  "name": "auto-mapping",
  "version": "1.3.2",
  "description": "map and convert object in typescript",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "mocha --require ts-node/register test/**/*.{ts,tsx}",
    "ts": "ts-node",
    "demo": "ts-node test/demo",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "fix": "tslint -c tslint.json 'src/**/*.ts' --fix",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run type-check&&npm run fix&&webpack",
    "pack": "npm run build&&npm pack"
  },
  "keywords": [
    "mapping",
    "mapper",
    "decorator",
    "convertion"
  ],
  "author": "fisher <i.yu@qq.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/fishen/auto-mapping.git"
  },
  "dependencies": {
    "reflect-metadata": "^0.1.13",
    "secure-template": "^1.0.2"
  },
  "devDependencies": {
    "@types/chai": "^4.2.9",
    "@types/mocha": "^5.2.7",
    "bundle-dts": "^1.1.2",
    "chai": "^4.2.0",
    "mocha": "^6.2.2",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tslint": "^5.20.1",
    "typescript": "^3.7.5",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2"
  }
}
